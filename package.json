{
  "name": "villain",
  "version": "0.0.1",
  "description": "An open source and touch friendly web based comic book reader",
  "main": "./build/villain.js",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.config.js --mode development --open --hot",
    "build": "rollup -c rollup.config.js",
    "watch": "rollup -c rollup.config.js -w",
    "format": "pretty-quick",
    "format:css": "npx prettier --config ./.prettierrc --write **/*.css.module"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/btzr-io/Villain.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Baltazar Gomez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/btzr-io/Villain/issues"
  },
  "homepage": "https://github.com/btzr-io/Villain#readme",
  "dependencies": {
    "clsx": "^1.0.4",
    "libarchive.js": "^1.1.0",
    "openseadragon": "^2.4.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "express": "^4.16.4",
    "husky": "^2.2.0",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "rollup": "^1.11.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "worker-loader": "^2.0.0"
  }
}
